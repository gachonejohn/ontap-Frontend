{
  "compilerOptions": {
    "ignoreDeprecations": "6.0",
    "baseUrl": "src",
    "paths": {
      "@components/*": ["components/*"],
      "@auth/*": ["auth/*"],
      "@dashboards/*": ["dashboards/*"],
      "@store/*": ["store/*"],
      "@schemas/*": ["schemas/*"],
      "@hooks/*": ["hooks/*"],
      "@utils/*": ["utils/*"],
      "@pages/*": ["pages/*"],
      "@constants/*": ["constants/*"]
    },
    "module": "commonjs",
    "target": "esnext",
    "jsx": "react-jsx"
  },
  "include": ["src"]
}
